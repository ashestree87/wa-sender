{
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --legacy-watch --watch server server/index.js",
    "build": "cd client && npm install && npm run build",
    "db:create": "wrangler d1 create wa_sender_db",
    "db:migrate": "wrangler d1 execute wa_sender_db --file=./db/schema.sql",
    "db:local": "better-sqlite3 wa_sender.db 'CREATE TABLE IF NOT EXISTS \"_cf_D1_version\" (\"version\" INTEGER NOT NULL)'",
    "db:shell": "sqlite3 wa_sender.db",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240508.0",
    "@databases/sqlite": "^4.1.0",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/forms": "^0.5.10",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-schedule": "^2.1.1",
    "openai": "^4.0.0",
    "qrcode": "^1.5.4",
    "react-router-dom": "^7.3.0",
    "uuid": "^11.1.0",
    "whatsapp-web.js": "^1.26.0"
  },
  "devDependencies": {
    "axios": "^1.8.2",
    "better-sqlite3": "^9.5.0",
    "nodemon": "^3.0.0",
    "sqlite3": "^5.1.6",
    "wrangler": "^3.39.0"
  }
}